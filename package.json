{
  "name": "clappr-markers-plugin",
  "version": "0.5.0",
  "description": "A plugin for clappr which will display markers at configured points scrub bar.",
  "main": "dist/clappr-markers-plugin.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --progress --optimize-occurence-order",
    "watch": "./node_modules/.bin/webpack --progress --optimize-occurence-order --watch",
