  "license": "MIT",
  "repository": "tjenkinson/clappr-markers-plugin",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "clean-webpack-plugin": "^0.1.8",
    "compass-mixins": "^0.12.7",
    "css-loader": "^0.23.1",
    "event-emitter": "^0.3.4",
    "node-sass": "^3.4.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14"
  }
}
